lang.name = FizzBuzz
lang.version = v1.0.0

$MAX = 50

$i = 0
con.until($i > $MAX)
	$3or5flag = 0
	
	con.if(!func.modi($i, 3))
		func.print(Fizz)
		$3or5flag = 1
	con.endif
	con.if(!func.modi($i, 5))
		func.print(Buzz)
		$3or5flag = 1
	con.endif
	con.if(!$3or5flag)
		func.print($i)
	con.endif
	func.println()
	
	$i += 1
con.endloop
